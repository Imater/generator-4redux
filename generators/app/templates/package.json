{
  "name": "<%= appName %>",
  "description": "<%= appDesc %>",
  "author": "<%= name %> <<%= email %>>",
  "keywords": "<%= appKeywords %>",
  "license": "<%= appLicense %>",
  "version": "<%= appVersion %>",
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "build": "concurrent --kill-others \"better-npm-run build\" \"npm run build-storybook\"",
    "lint": "eslint -c .eslintrc src api --ext js,jsx",
    "lint-fix": "eslint -c .eslintrc src api --ext js,jsx --fix",
    "start-dev": "better-npm-run start-dev",
    "start-dev-render-on-client": "better-npm-run start-dev-render-on-client",
    "start-dev-api": "better-npm-run start-dev-api",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "dev-render-on-client": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev-render-on-client\" \"npm run start-dev-api\"",
    "test": "better-npm-run test",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*.test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*.test.js') --compilers js:babel-core/register --watch",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "better-npm-run build-storybook"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node --harmony ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3031
      }
    },
    "start-prod-api": {
      "command": "node --harmony ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3031
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3031
      }
    },
    "start-dev-render-on-client": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "__DISABLE_SSR__": true,
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3031
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3031
      }
    },
    "watch-client": {
      "command": "node ./bin/webpack.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src/"
      }
    },
    "build": {
      "command": "webpack --color --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-storybook": {
      "command": "build-storybook -o static/__components",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "dependencies": {
    "axios": "^0.13.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.1",
    "classnames": "^2.2.3",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.2",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "http-proxy": "^1.12.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "method-override": "^2.3.5",
    "phantomjs-prebuilt": "2.1.10",
    "pretty-error": "^2.0.0",
    "promise-waterfall": "^0.1.0",
    "pure-render-decorator": "^1.1.1",
    "querystring": "^0.2.0",
    "ramda": "^0.22.0",
    "raw-loader": "^0.5.1",
    "react": "^15.3.0",
    "react-addons-perf": "^15.3.0",
    "react-cookie": "^0.4.6",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.1.0",
    "react-inline-css": "^2.0.0",
    "react-redux": "^4.0.0",
    "react-router": "2.6.1",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.5",
    "recompose": "0.20.2",
    "redux": "^3.5.2",
    "redux-act": "^0.5.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-form": "^5.3.2",
    "redux-loop": "^2.1.1",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "url-loader": "^0.5.7",
    "urlencode": "^1.1.0",
    "warning": "^3.0.0",
    "webpack-isomorphic-tools": "^2.2.18",
    "yeoman-generator": "0.24.1"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.1.1",
    "@kadira/storybook": "^1.41.0",
    "autoprefixer-loader": "^3.1.0",
    "babel-eslint": "^6.1.0",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-typecheck": "^3.6.0",
    "better-npm-run": "^0.0.10",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.6",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "3.2.2",
    "eslint-config-airbnb": "10.0.0",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^1.3.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "json-loader": "^0.5.4",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-istanbul-reporter": "0.0.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.1",
    "node-sass": "^3.4.2",
    "phantomjs-polyfill": "0.0.2",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.3.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "sass-loader": "^4.0.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.2",
    "stylus-loader": "^2.1.1",
    "webpack": "^2.1.0-beta.14",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "engines": {
    "node": "5.6.0"
  }
}
